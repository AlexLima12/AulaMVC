@model IEnumerable<Proj.Models.Cidade>
<h1>
    Todos os dados
</h1>
<table class="table">
        <tr>
            <th>Id</th>
            <th>Nome da cidade</th>
            <th>Estado</th>
            <th>Habitantes</th>
        </tr>
        @{
            foreach(var item in Model) {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                    <td>@item.Estado</td>
                    <td>@item.Habitantes Habitantes</td>
                </tr>
            }
        }
</table>