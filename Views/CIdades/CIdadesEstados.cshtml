@model IEnumerable<Proj.Models.Cidade>
<h1>
    Minhas Cidades e Seus Estados
</h1>
<table class="table">
    <tr>
        <th>Nome da cidade</th>
        <th>Estado</th>
    </tr>
    @{
        foreach(var item in Model) {
            <tr>
                <td>@item.Nome</td>
                <td>@item.Estado</td>
            </tr>
        }
    }
</table>